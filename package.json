{
    "name": "nice2",
    "version": "0.0.1",
    "description": "React Nice UI kit with theme support",
    "main": "dist/nice2.min.js",
    "scripts": {
        "storybook": "start-storybook -s ./stories/static -p 9001 -c .storybook",
        "storybook:prod": "build-storybook -s ./stories/static -c .storybook",
        "build": "webpack --config ./etc/webpack/webpack.dev.config.js",
        "build:prod": "webpack --config ./etc/webpack/webpack.prod.config.js",
        "lint": "npm run lint:ci -- --fix",
        "lint:ci": "eslint .",
        "lint-styles": "stylelint 'src/**/*.s(a|c)ss'",
        "test": "jest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run test && npm run lint && npm run lint-styles"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/textkernel/nice2.git"
    },
    "author": "Textkernel Nice People <nice@textkernel.nl>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/textkernel/nice2/issues"
    },
    "homepage": "https://github.com/textkernel/nice2#readme",
    "dependencies": {
        "@babel/polyfill": "7.0.0",
        "diacritics": "1.3.0",
        "prop-types": "15.6.2",
        "react": "16.6.3",
        "react-dom": "16.6.3"
    },
    "devDependencies": {
        "@babel/cli": "7.1.0",
        "@babel/core": "7.1.0",
        "@babel/plugin-proposal-class-properties": "7.1.0",
        "@babel/plugin-proposal-decorators": "7.1.0",
        "@babel/preset-env": "7.1.0",
        "@babel/preset-react": "7.0.0",
        "@storybook/addon-info": "4.0.8",
        "@storybook/addon-knobs": "4.0.12",
        "@storybook/react": "4.0.8",
        "autoprefixer": "9.1.5",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.0",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.2",
        "css-loader": "1.0.0",
        "enzyme": "3.7.0",
        "enzyme-adapter-react-16": "1.7.0",
        "enzyme-to-json": "3.3.5",
        "eslint": "5.6.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-prettier": "3.1.0",
        "eslint-loader": "2.1.1",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-jsx-a11y": "6.1.1",
        "eslint-plugin-prettier": "2.7.0",
        "eslint-plugin-react": "7.11.1",
        "html-webpack-plugin": "3.2.0",
        "husky": "1.2.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "23.6.0",
        "mini-css-extract-plugin": "0.4.3",
        "node-sass": "4.9.3",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "postcss-import": "12.0.0",
        "postcss-loader": "3.0.0",
        "precss": "3.1.2",
        "prettier": "1.14.3",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.0",
        "stylelint": "9.6.0",
        "stylelint-config-recommended": "2.1.0",
        "stylelint-config-recommended-scss": "3.2.0",
        "stylelint-order": "1.0.0",
        "stylelint-scss": "3.3.1",
        "stylelint-webpack-plugin": "0.10.5",
        "uglifyjs-webpack-plugin": "2.0.1",
        "webpack": "4.28.1",
        "webpack-cli": "3.2.1",
        "webpack-dev-server": "3.1.14"
